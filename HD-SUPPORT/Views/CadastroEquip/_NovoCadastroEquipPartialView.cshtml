@model HD_SUPPORT.Models.CadastroEquip
@inject IHttpContextAccessor Acessor

@{
    ViewData["Title"] = "Cadastro";
}


                <form asp-action="NovoCadastro">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <input type="hidden" asp-for="profissional_HD" value="@Acessor.HttpContext.Session.GetString("profissional") - @Acessor.HttpContext.Session.GetString("nome")" />
                    <div class="form-group">
                        <h5 class="control-label">Id do patrimônio</h5>
                        <input asp-for="IdPatrimonio" class="form-control" />
                        <span asp-validation-for="IdPatrimonio" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <h5 class="control-label">Modelo</h5>
                        <input asp-for="Modelo" class="form-control" />
                        <span asp-validation-for="Modelo" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <h5 class="control-label">Processador</h5>
                        <input asp-for="Processador" class="form-control" />
                        <span asp-validation-for="Processador" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <h5 class="control-label">Sistema operacional</h5>
                        <input asp-for="SistemaOperacionar" class="form-control" />
                        <span asp-validation-for="SistemaOperacionar" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <h5 class="control-label">Headset</h5>
                        <input asp-for="HeadSet" class="form-control" />
                        <span asp-validation-for="HeadSet" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <input type="submit" value="Adicionar" class="btn btn-primary" />
                    </div>
                </form>